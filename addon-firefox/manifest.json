{

    "description": "Firefox addon to save Tiddlywiki",
    "manifest_version": 2,
    "name": "timimi",
    "version": "2.0.3",
    "icons":
    {
        "16": "icons/index.svg",
        "32": "icons/index.svg",
        "48": "icons/index.svg",
        "96": "icons/index.svg"
    },
    "applications":
    {
        "gecko":
        {
            "id": "timimi@tesseract.io",
            "strict_min_version": "57.0"
        }
    },
    "background":
    {
        "scripts": ["background.js"]
    },
    "browser_action":
    {
        "browser_style": true,
        "default_icon": "icons/index.svg"
    },
    "content_scripts": [
    {
        "matches": ["file:///*"],
        "js": ["content-script.js"]
    }],
    "options_ui": { 
         "page": "settings/options.html" 
     },
    "permissions": [
        "nativeMessaging",
        "storage",
        "notifications"
    ]
}
